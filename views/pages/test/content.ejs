<script id="table_template" type="text/html">
    <table border="1">
        <thead>
            <tr>
                <% $.each(data[0], function(key,val){%>
                <th><%=key%></th>
                <% });%>
            </tr>
        </thead>
        <tbody>
            <% $.each(data, function(rowindex,row) {%>
            <tr>
                <% $.each(row, function(key,val){%>
                <td><%= val%></td>
                <% });%>
            </tr>
            <%}); %>
        </tbody>
    </table>
</script>


<div style="direction:ltr;">
	<div>
		apiType: <input id="inpApiatype" value="account_api" />
	</div>
	<div>
		method: <input id="inpMethod" value="get_client_by_term" />
	</div>
	<div>
		params: <textarea id="tarParams">{"term":"גלב"}</textarea>
	</div>
	<div id="results"></div>
	<button id="btnRunTest" onclick="runTest()" >Run</button>
</div>

