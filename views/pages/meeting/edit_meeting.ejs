<script id="edit_meeting" type="text/html">
   <form id="editMeeting" name="editMeeting">
     
     
        <input type="hidden" name="serial" value="<%=data.main[0].Serial %>" />
        <input type="hidden" name="client_serial" value="<%=data.main[0].ClientSerial %>" />
       <input type="button" id="btn_save_meeting"  name="btn_save_meeting"  onclick  ="save_meeting()" value="save" />
      

    <table  id="document_table">
        
            <tr>
                <td >
                    <div>תאריך פגישה</div>
                     <div >
                          <input name="meeting_date" id="meeting_date" value=" <%=data.main[0].MeetingDate %>" size="10"  />
                     </div>
                </td>
                
                <td>
                    <div>מכין התיק</div>
                     <div>
                             <select name="editor_name">
                                        <option value="0" > </option>
                                       <%    $.each(data.user_name_list, function(key,val){
                                             if(val.Serial != data.main[0].EditorName){ %>
                                                   <option value="<%=val.Serial%>" ><%=val.UserName%></option>
                     
                                           <%}
                                              else{ %>
                                                      <option value="<%=val.Serial%>" selected="selected" ><%=val.UserName%></option>
                                           <%  }
                                     });%>
                      </select>
                     </div>
                </td>
                 <td>
                    <div>שם סוכן</div>
                     <div>
                             <select name="agent_name">
                                        <option value="0" > </option>
                                       <%    $.each(data.agent_name_list, function(key,val){
                                             if(val.Serial != data.main[0].AgentName){ %>
                                                   <option value="<%=val.Serial%>" ><%=val.AgentName%></option>
                     
                                           <%}
                                              else{ %>
                                                      <option value="<%=val.Serial%>" selected="selected" ><%=val.AgentName%></option>
                                           <%  }
                                     });%>
                      </select>
                     </div>
                </td>
                 <td>
                    <div>סטטוס פגישה</div>
                     <div>
                             <select name="meeting_status">
                                        <option value="0" > </option>
                                       <%    $.each(data.meeting_status_list, function(key,val){
                                             if(val.Serial != data.main[0].MeetingStatus){ %>
                                                   <option value="<%=val.Serial%>" ><%=val.ParamName%></option>
                     
                                           <%}
                                              else{ %>
                                                      <option value="<%=val.Serial%>" selected="selected" ><%=val.ParamName%></option>
                                           <%  }
                                     });%>
                      </select>
                     </div>
                </td>
                  
            </tr>
        
           
     </table >  
          <textarea name="meeting_summary"  style="width:100% ; height : 400px ;" >
                   <%= data.main[0].MeetingSummary %>
            </textarea>
         <a href="#" onclick="for_each() ">for each</a>
   </form>  
</script>